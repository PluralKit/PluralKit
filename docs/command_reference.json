{
	"commands":[
		{
			"key":"SystemInfo",
			"usage":"system [system]",
			"description":"Looks up information about a system"
		},
		{
			"key":"SystemNew",
			"usage":"system new [name]",
			"description":"Creates a new system"
		},
		{
			"key":"SystemRename",
			"usage":"system rename [new name]",
			"description":"Renames your system"
		},
		{
			"key":"SystemDesc",
			"usage":"system description [description]",
			"description":"Changes your system's description"
		},
		{
			"key":"SystemTag",
			"usage":"system tag [tag]",
			"description":"Changes your system's tag"
		},
		{
			"key":"SystemAvatar",
			"usage":"system avatar [url|@mention]",
			"description":"Changes your system's avatar image"
		},
		{
			"key":"SystemDelete",
			"usage":"system delete",
			"description":"Deletes your system"
