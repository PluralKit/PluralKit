		{
			"key":"PermCheck",
			"usage":"permcheck [server id]",
			"description":"[Checks the given server's permission setup](<https://pluralkit.me/staff/permissions/#permission-checker-command>) to check if it's compatible with PluralKit"
		},
		{
			"key":"Stats",
			"usage":"stats",
			"description":"Prints statistics related to PluralKit, such as resource usage, latency, and registered system/member count"
		}
